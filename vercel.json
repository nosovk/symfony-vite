{
	"version": 2,
	"builds": [
		{
			"src": "/api/index.php",
			"use": "vercel-php@0.7.3",
			"config": {
				"excludeFiles": "{public/build/**,public/bundles/**,public/css/**,public/tinymce/**,public/uploads/**,assets/**}"
			}
		},
		{ "src": "/public/**", "use": "@vercel/static" }
	],
	"routes": [
		{
			"src": "/(css|js)/(.*)",
			"dest": "public/$1/$2"
		},
		{
			"src": "/(.*)",
			"dest": "/api/index.php"
		}
	],
	"buildCommand": "npm run build",
	"devCommand": "npm run dev",
	"outputDirectory": "."
}
